#задача Соседи одного знака
a = input().split() 
for i in range(len(a)-1): 
    n = int(a[i])  
    #i += 1
    m = int(a[i+1])  
    if (n * m) > 0:
        print(n, m, end=';')
        break
#задача Наибольший элемент
max = 0
a = [int(i) for i in input().split()]
for i in range(1, len(a)):
    if a[i] > a[max]:
        max = i
print(a[max], max)
